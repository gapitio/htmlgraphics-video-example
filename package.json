{
  "name": "htmlgraphics-svelte-bundler-template",
  "version": "0.0.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "run-s validate build:prod build:custom-properties build:panel-options",
    "build:custom-properties": "node --experimental-json-modules ./scripts/customProperties.js",
    "build:dev": "rollup -c rollup.config.dev.js",
    "build:prod": "rollup -c rollup.config.prod.js ",
    "build:panel-options": "node ./scripts/panelOptions.js",
    "dev": "run-p watch serve:dev",
    "fix": "run-s fix:eslint fix:prettier",
    "fix:eslint": "eslint . --ext .ts,.js,.cjs,.svelte --fix",
    "fix:prettier": "prettier --write .",
    "lint": "run-s lint:eslint lint:prettier",
    "lint:eslint": "eslint . --ext .ts,.js,.cjs,.svelte",
    "lint:prettier": "prettier --check .",
    "serve": "sirv public --single --host",
    "serve:dev": "sirv public --single --host --dev",
    "typecheck": "tsc --noEmit",
    "validate": "run-p lint typecheck",
    "watch": "rollup -c rollup.config.dev.js -w"
  },
  "devDependencies": {
    "@gapit/format-si": "0.0.1",
    "@gapit/grafana-metric": "1.0.0",
    "@grafana/data": "~7.1.5",
    "@rollup/plugin-commonjs": "^18.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.2.1",
    "@tsconfig/svelte": "^1.0.10",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-svelte3": "^3.1.2",
    "eslint-plugin-unicorn": "^29.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.44.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "sass": "^1.32.8",
    "sirv-cli": "^1.0.11",
    "svelte": "^3.37.0",
    "svelte-check": "^1.4.0",
    "svelte-preprocess": "^4.7.0",
    "tslib": "^2.2.0",
    "typescript": "^4.2.3"
  }
}
